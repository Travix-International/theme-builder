{
  "name": "theme-builder",
  "version": "1.0.0",
  "description": "README",
  "main": "dist/app.js",
  "scripts": {
    "build": "babel --copy-files ./src --out-dir dist",
    "install": "npm run build",
    "test": "jest",
    "test:update": "jest -u"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:Travix-International/theme-builder.git"
  },
  "keywords": [
    "themes",
    "builder",
    "yaml",
    "themes-builder"
  ],
  "author": "Ivana Viranovska",
  "license": "MIT",
  "homepage": "https://github.com/Travix-International/theme-builder#readme",
  "jest": {
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(js)$",
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "collectCoverage": true,
    "testEnvironment": "node"
  },
  "devDependencies": {
    "babel-cli": "6.11.4",
    "babel-core": "6.13.2",
    "babel-eslint": "6.1.2",
    "babel-jest": "16.0.0",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.16.0",
    "eslint": "3.2.2",
    "eslint-config-travix": "1.1.0",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-import": "1.12.0",
    "eslint-plugin-react": "5.2.2",
    "jest": "16.0.2",
    "webpack": "1.13.1"
  },
  "dependencies": {
    "babel-register": "6.11.6",
    "js-yaml": "3.6.1"
  }
}
